<h3>{{viewTitle}}</h3>
<div class="container" id="abc">
   
    <div class="row">
        <div class="col-md-6">
    
  <form action="/user/addProcess" autocomplete="off">
  
    <div class="row">
      <div class="col-20">
        <label for="fname">1. Client Name</label>
      </div>
      <div class="col-75">
        <input type="text-center"  class="form-control" id="cname" maxlength="30" name="clientName" value="{{process.clientName}}" readonly>
      </div>
    </div>
    <div class="row">
      <div class="col-20">
        <label for="lname">2. Business Unit</label>
      </div>
      <div class="col-75">
        <input type="text-center" class="form-control" id="busiunit" maxlength="30" name="Buss_Unit" value="{{process.Buss_Unit}}">
      </div>
    </div>
    <div class="row">
      <div class="col-20">
        <label for="country">3. Sub Business Unit</label>  
      </div>
      <div class="col-75">
        <input type="text-center" class="form-control" id="subbunit" maxlength="30" name="Sub_Buss_Unit" value="{{process.Sub_Buss_Unit}}">
      </div>
    </div>
    <div class="row">
        <div class="col-20">
          <label for="country">4. Process Name</label>  
        </div>
        <div class="col-75">
          <input type="text-center" class="form-control" id="pname" maxlength="30" name="Proc_Name" value="{{process.Proc_Name}}">
        </div>
      </div>
      <div class="row">
        <div class="col-20">
          <label for="country">5. Process Id</label>  
        </div>
        <div class="col-75">
          <input type="text-center"  class="form-control" id="procid" maxlength="30" name="Proc_Id" value="{{process.Proc_Id}}">
        </div>
      </div>
    <div class="row">
      <div class="col-20">
        <label for="subject">6. Process Description</label>
      </div>
      <div class="col-75">
        <input id="subject" class="form-control"  name="Proc_Desc" style="height:120px" value="{{process.Proc_Desc}}">
      </div>
    </div>
    <div class="row">
        <div class="col-20">
          <label for="country">7. Monthly Volume</label>  
        </div>
        <div class="col-75">
            <input type="number-center" class="form-control" oninput="calculate()"   id="mv" maxlength="30" name="Mon_Vol" value="{{process.Mon_Vol}}"/>
        </div>
      </div>
      <div class="row">
        <div class="col-20">
          <label for="country">8. AHT(in minutes)</label>  
        </div>
        <div class="col-75">
            <input type="number-center" class="form-control" oninput="calculate()"   id="aht" step="any" maxlength="30" name="AHT" value="{{process.AHT}}"/>
        </div>
      </div>
      <div class="row">
        <div class="col-20">
          <label for="country">9. FTE</label>  
        </div>
        <div class="col-75">
            <input type="number-center"   class="form-control"  id="fte" step="any" maxlength="30" name="FTE" value="{{process.FTE}}">
        </div>
      </div>
      <div class="row">
        <div class="col-20">
          <label for="sla">10. SLA</label>
        </div>
        <div class="col-75">
          <select  class="form-control" id="slacalc" name="SLA">
                <option>{{process.SLA}}</option>
                
          </select>
        </div>
      </div>
      
    </div>
    
    <div class="col-md-6">
    <div class="row">
        <div class="col-20">
          <label for="fname">11. TAT</label>
        </div>
        <div class="col-75">
          <input  class="form-control" type="text-center" id="tatname" maxlength="50" name="TAT" value="{{process.TAT}}">
        </div>
      </div>
      <div class="row">
          <div class="col-20">
            <label for="lname">12. Applications Used</label>
          </div>
          <div class="col-75">
            <input  class="form-control" type="text-center" id="appused" maxlength="150" name="App_Used" value="{{process.App_Used}}">
          </div>
        </div>
        <div class="row">
            <div class="col-20">
              <label for="country">13. Documentations of the process</label>  
            </div>
            <div class="col-75">
                  <select  class="form-control" id="docpre" name="Doc_Present" >
                          <option>{{process.Doc_Present}}</option>
                          </select>
            </div>
          </div>
          <div class="row">
              <div class="col-20">
                <label for="country">14. Rule based process</label>  
              </div>
              <div class="col-75">
                      <select class="form-control"  id="rbased" name="Rule_Based">
                             <option>{{process.Rule_Based}}</option>
                             </select>
              </div>
            </div>
          <div class="row">
            <div class="col-20">
              <label for="subject">15. Structure of Input Data</label>
            </div>
            <div class="col-75">
                  <select class="form-control" id="dattype" name="Stuc_Data">
                          <option>{{process.Stuc_Data}}</option>
                     </select>
            </div>
          </div>
          <div class="row">
              <div class="col-20">
                <label for="country">16. Input data type</label>  
              </div>
              <div class="col-75">
                      <select  class="form-control" id="indat" name="Inp_Data_Type">
                               <option>{{process.Inp_Data_Type}}</option>
                         </select>
              </div>
            </div>
            <div class="row">
              <div class="col-20">
                <label for="country">17. Percentage of process amenable for RPA</label>  
              </div>
              <div class="col-75">
                      <input class="form-control" type="number-center" step="any" name="Amenable_RPA" value="{{process.Amenable_RPA}}">
              </div>
            </div>
            <div class="row">
              <div class="col-20">
                <label for="country">18. Percentage of process amenable for cognitive</label>  
              </div>
              <div class="col-75">
                      <input class="form-control" type="number-center" step="any" name="Amenable_Cognitive" value="{{process.Amenable_Cognitive}}">
              </div>
            </div>
            <div class="row">
              <div class="col-20">
                <label for="sla">19. Automation Readiness</label>
              </div>
              <div class="col-75">
                      <input class="form-control" type="number-center" step="any" name="Automation_Ready" value="{{process.Automation_Ready}}">
              </div>
            </div>
            <div class="row">
              <div class="col-20">
                <label for="sla">20. Percentage of Automation Potential</label>
              </div>
              <div class="col-75">
                      <input class="form-control" type="number-center" step="any" name="AP_Perc" value="{{process.AP_Perc}}">
              </div>
            </div>
            <div class="row">
              <div class="col-20">
                <label for="sla">21. FTE Benifit</label>
              </div>
              <div class="col-75">
                      <input class="form-control" type="number-center" step="any" name="FTE_Benefit" value="{{process.FTE_Benefit}}">
              </div>
            </div>
            
            
          </div>
          </div>
          <div class="text-center">
          
          
                
            <input type="submit" class="btn btn-success" value="Submit"> 
        
          </div>
          </div>
          
             
          
        </div>
        </div>
          
  </form>
  
